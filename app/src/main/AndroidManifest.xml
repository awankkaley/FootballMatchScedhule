<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.kotlin.awankkaley.footballmatchscedhule">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".feature.details.DetailActivity">
        </activity>
        <activity android:name=".detailteam.DetailTeamActivity">
        </activity>
        <activity
                android:name=".detailteam.ScrollingDetailActivity"
                android:label="@string/title_activity_scrolling_detail"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".feature.detailteam.ScrollingDetailsActivity"
                android:label="@string/title_activity_scrolling_details"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".feature.detailteam.ScrollingPlayerDetailActivity"
                android:label="@string/title_activity_scrolling_player_detail"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".search.SearchMatchActivity">
        </activity>
        <activity android:name=".feature.match.search.SearchMatchMatchActivity">
        </activity>
    </application>

</manifest>